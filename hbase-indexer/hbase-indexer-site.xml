<?xml version="1.0"?>
<configuration>
  <property>
    <name>hbaseindexer.zookeeper.session.timeout</name>
    <value>30000</value>
  </property>

<configuration>
   <property>
      <name>hbaseindexer.zookeeper.connectstring</name>
      <value>{HostName}:2181</value>
   </property>
   <property>
      <name>hbase.zookeeper.quorum</name>
      <value>{HostName}</value>
   </property>
</configuration>
